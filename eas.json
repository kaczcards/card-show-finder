{
  "cli": {
    "version": ">= 16.14.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "cache": {
        "key": "dev-${{ github.sha }}"
      },
      "env": {
        "APP_ENV": "development"
      },
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": "buildNumber",
        "scheme": "cardshowfinderDev"
      },
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "resourceClass": "medium",
        "autoIncrement": false,
        "credentialsSource": "remote",
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "developmentClient": false,
      "channel": "preview",
      "env": {
        "APP_ENV": "preview"
      },
      "cache": {
        "key": "preview-${{ github.sha }}"
      },
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": "buildNumber",
        "scheme": "cardshowfinderPreview"
      },
      "android": {
        "resourceClass": "large",
        "autoIncrement": false,
        "credentialsSource": "remote",
        "buildType": "apk"
      }
    },
    "staging": {
      "distribution": "internal",
      "developmentClient": false,
      "channel": "staging",
      "env": {
        "APP_ENV": "staging"
      },
      "cache": {
        "key": "staging-${{ github.sha }}",
        /* default cache paths are applied automatically;
           deprecated flag removed per Expo docs */
      },
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": "buildNumber",
        "scheme": "cardshowfinderStaging"
      },
      "android": {
        "resourceClass": "large",
        "autoIncrement": false,
        "credentialsSource": "remote",
        "buildType": "apk"
      }
    },
    "production": {
      "channel": "production",
      "developmentClient": false,
      "env": {
        "APP_ENV": "production"
      },
      "cache": {
        "key": "prod-${{ eas.build.id }}"
      },
      "ios": {
        "resourceClass": "large",
        "autoIncrement": "buildNumber",
        "scheme": "cardshowfinder"
      },
      "android": {
        "resourceClass": "large",
        "autoIncrement": false,
        "credentialsSource": "remote",
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "staging": {
      "ios": {
        "appleId": "$APPLE_ID",
        "ascAppId": "$ASC_APP_ID",
        "appleTeamId": "$APPLE_TEAM_ID"
      },
      "android": {
        "serviceAccountKeyPath": "./android-service-account.json",
        "track": "internal"
      }
    },
    "production": {
      "ios": {
        "appleId": "$APPLE_ID",
        "ascAppId": "$ASC_APP_ID",
        "appleTeamId": "$APPLE_TEAM_ID"
      },
      "android": {
        "serviceAccountKeyPath": "./android-service-account.json",
        "track": "production"
      }
    }
  }
}
