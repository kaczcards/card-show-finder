{
  "name": "card-show-finder",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean": "expo start -c",
    "geocode": "node geocode-shows.js",
    "scraper": "node scraper/index.js",
    "scraper:help": "node scraper/index.js --help",
    "scraper:tx": "node scraper/index.js --state TX",
    "scraper:csv": "node scraper/run-csv-batch.js",
    "scraper:csv:help": "node scraper/run-csv-batch.js --help",
    "scraper:csv:in": "node scraper/run-csv-batch.js --state IN",
    "scraper:inspect": "node scraper/inspect-pending-shows.js",
    "scraper:inspect:stats": "node scraper/inspect-pending-shows.js --stats-only",
    "verify-backups": "node scripts/verify_backup_status.js",
    "db:rls:apply": "node apply-consolidated-rls.js",
    "db:rls:verify": "node apply-consolidated-rls.js --verify-only",
    "db:rls:apply-only": "node apply-consolidated-rls.js --apply-only",
    "test:security": "node simple-security-test-runner.js",
    "test:security:verbose": "node simple-security-test-runner.js",
    "test:db:security": "node simple-security-test-runner.js",
    "test:e2e": "detox test --configuration ios.sim.debug",
    "test:e2e:build": "detox build --configuration ios.sim.debug",
    "test:e2e:clean": "detox clean-framework-cache && detox build --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:android:build": "detox build --configuration android.emu.debug",
    "test:e2e:batch": "node e2e/scripts/run-batch.js",
    "test:e2e:all": "node e2e/scripts/run-all-batches.js",
    "test:e2e:auth": "npm run test:e2e:batch -- --batch auth-basic",
    "test:e2e:auth-advanced": "npm run test:e2e:batch -- --batch auth-advanced",
    "test:e2e:tag": "npm run test:e2e:batch -- --tag",
    "test:e2e:core": "npm run test:e2e:batch -- --batch core",
    "test:e2e:edge": "npm run test:e2e:batch -- --batch edge",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 1200",
    "lint:src": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:src:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix --max-warnings 0",
    "lint:src:check": "npm run lint:src",
    "lint:simple-fix": "node scripts/simple-lint-fix.js",
    "lint:recovery": "node scripts/lint-recovery.js",
    "lint:safe-fix": "node scripts/safe-lint-fixes.js",
    "unused:check": "node scripts/unused-cleanup.js --dry-run --verbose --report-files",
    "unused:fix-imports": "node scripts/unused-cleanup.js --fix-imports --report-files",
    "unused:cleanup": "node scripts/unused-cleanup.js --fix-imports --clean-files --report-files",
    "unused:report": "node scripts/unused-cleanup.js --dry-run --report-files --verbose",
    "auto-fix:unused": "node scripts/automated-unused-fixes.js",
    "auto-fix:dry-run": "node scripts/automated-unused-fixes.js --dry-run --verbose",
    "typecheck": "tsc --noEmit",
    "test:lint": "npm run lint:src:check",
    "test:type": "npm run typecheck",
    "test:unit": "npx jest __tests__/smoke.unit.test.ts",
    "test:db:unit": "npm run db:rls:verify && pg_prove test/database/**/*.sql",
    "test:api": "jest --runInBand --selectProjects api",
    "test:perf": "node e2e/scripts/run-batch.js --batch perf --perf",
    "test:sec": "npm audit --audit-level=high && gitleaks detect --no-banner",
    "test:build:verify": "scripts/test-build-verify.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:fast": "npm-run-all -s test:lint test:type test:unit",
    "test:ci": "npm-run-all -s test:lint test:type test:unit test:db:unit test:api test:db:security",
    "test:full": "npm-run-all -s test:ci test:e2e:auth test:e2e:core test:e2e:edge test:perf test:sec",
    "test:segments": "node scripts/test-segments.js",
    "build:web": "expo export:web",
    "preview": "expo start --no-dev --minify"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^7.3.15",
    "@react-navigation/material-top-tabs": "^7.3.2",
    "@react-navigation/native": "^7.1.11",
    "@react-navigation/native-stack": "^7.3.21",
    "@react-navigation/stack": "^7.4.2",
    "@stripe/stripe-react-native": "0.45.0",
    "@supabase/supabase-js": "^2.38.0",
    "@tanstack/react-query": "^5.82.0",
    "chalk": "^4.1.2",
    "console-table-printer": "^2.12.1",
    "csv-parse": "^6.1.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "dateformat": "^4.6.3",
    "dotenv": "^17.2.0",
    "expo": "53.0.20",
    "expo-application": "~6.1.5",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "^7.1.4",
    "expo-haptics": "^14.1.4",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-location": "~18.1.6",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "inquirer": "^8.2.6",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "open": "^8.4.2",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-maps": "1.20.1",
    "react-native-pager-view": "6.7.1",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-tab-view": "^4.1.2",
    "react-native-toast-message": "^2.3.3",
    "react-native-vector-icons": "^10.2.0",
    "readline": "^1.3.0",
    "sentry-expo": "~7.0.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/react": "~19.0.10",
    "@types/react-native-maps": "^0.24.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^30.0.5",
    "mockdate": "^3.0.5",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3"
  },
  "overrides": {
    "@sentry/browser": ">=7.119.1",
    "@sentry/react": ">=7.119.1",
    "@sentry/react-native": ">=5.34.0"
  },
  "private": true,
  "description": "Cross-platform (iOS & Android) mobile app for discovering **trading-card shows** happening near you.   Built with **React Native (Expo)** + **Supabase** so one code-base runs everywhere.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaczcards/card-show-finder.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/kaczcards/card-show-finder/issues"
  },
  "homepage": "https://github.com/kaczcards/card-show-finder#readme"
}
